            "news_api",
            "twitter"
        ],
        "news_api": {
            "enabled": true,
            "keywords": [
                "stock",
                "market",
                "finance",
                "economy"
            ]
        },
        "twitter": {
            "enabled": false
        }
    },
    "corporate_actions": {
