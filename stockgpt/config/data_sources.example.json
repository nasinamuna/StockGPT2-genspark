            "enabled": true
        },
        "moneycontrol": {
            "enabled": true,
            "mapping": {
                "RELIANCE": "reliance-industries-ltd",
                "TCS": "tata-consultancy-services-ltd",
                "INFY": "infosys-ltd"
            }
        }
    },
    "financial_statements": {
        "sources": [
            "moneycontrol"
        ],
        "moneycontrol": {
            "enabled": true
        }
    },
    "news_social": {
